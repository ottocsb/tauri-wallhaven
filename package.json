{
  "name": "thaven",
  "version": "0.1.0",
  "description": "基于tauri的wallhaven",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vite build",
    "prepare": "husky install",
    "dev:host": "vite --host",
    "dev:open": "vite --open",
    "preview": "vite preview",
    "coverage": "vitest --coverage",
    "preinstall": "npx only-allow pnpm",
    "typecheck": "npx vue-tsc --noEmit",
    "preview:host": "vite preview --host",
    "preview:open": "vite preview --open",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "build:debug": "cross-env NODE_ENV=debug vite build"
  },
  "engines": {
    "node": ">=v18.17.0"
  },
  "packageManager": "pnpm@8.7.0",
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@intlify/unplugin-vue-i18n": "^6.0.1",
    "@tauri-apps/api": "^1.5.3",
    "@tauri-apps/cli": "1",
    "@types/ityped": "^1.0.3",
    "@types/node": "^18.17.12",
    "@typescript-eslint/parser": "^8.18.1",
    "@unocss/eslint-config": "^0.65.2",
    "@unocss/reset": "^0.65.2",
    "@vicons/ionicons5": "^0.13.0",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vueuse/components": "^12.0.0",
    "@vueuse/core": "^12.0.0",
    "changelogen": "^0.5.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "ityped": "^1.0.3",
    "lightningcss": "^1.28.2",
    "lint-staged": "^15.2.11",
    "local-pkg": "^0.5.1",
    "naive-ui": "^2.40.4",
    "perfect-debounce": "^1.0.0",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "typescript": "^5.7.2",
    "unocss": "^0.65.2",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.28.0",
    "unplugin-vue-router": "^0.10.9",
    "vite": "^6.0.11",
    "vite-auto-import-resolvers": "^3.2.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-env-types": "^0.1.4",
    "vite-plugin-removelog": "^0.2.2",
    "vite-plugin-use-modules": "^1.4.8",
    "vite-plugin-warmup": "^0.1.0",
    "vue": "^3.5.13",
    "vue-dark-switch": "^1.0.6",
    "vue-i18n": "^11.1.0",
    "vue-request": "2.0.4",
    "vue-router": "^4.5.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix"
  },
  "overrides": {
    "vue-demi": "npm:vue-demi@latest",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@latest",
    "array-includes": "npm:@nolyfill/array-includes@latest",
    "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@latest",
    "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
    "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
    "arraybuffer.prorotype.slice": "npm:@nolyfill/arraybuffer.prorotype.slice@latest",
    "function.prototype.name": "npm:@nolyfill/function.prototype.name@latest",
    "has": "npm:@nolyfill/has@latest",
    "object-keys": "npm:@nolyfill/object-keys@latest",
    "object.assign": "npm:@nolyfill/object.assign@latest",
    "object.entries": "npm:@nolyfill/object.entries@latest",
    "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
    "object.values": "npm:@nolyfill/object.values@latest"
